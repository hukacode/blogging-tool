{
  "name": "garden-helper",
  "displayName": "Garden Helper",
  "description": "A garden helper",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.56.0"
  },
  "publisher": "Huka",
  "repository": {
    "type": "git",
    "url": "https://github.com/hukacode/garden-helper.git"
  },
  "homepage": "https://github.com/hukacode/garden-helper/blob/main/README.md",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "garden-helper.changeUpdatedDate",
        "title": "garden-helper: Change Updated Date"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.56.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  }
}
